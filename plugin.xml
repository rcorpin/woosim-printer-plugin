<?xml version="1.0" encoding="UTF-8"?>
 
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="com.rcorpin.woosimPrinter"
        version="0.1.0">
    <name>WoosimPrinter</name>
    <description>Woosim Printer Cordova Plugin</description>
    <license>All Rights Reserved</license>
    <keywords>phonegap,cordova,woosim,printer,bluetooth</keywords>
 
 
    <js-module src="www/woosimPrinter.js" name="woosimPrinter">
        <clobbers target="woosimPrinter" />
    </js-module>
 
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="WoosimPrinter">
                <param name="android-package" value="WoosimPrinter"/>
            </feature>
        </config-file>
 
        <source-file src="src/android/WoosimPrinter.java" target-dir="src/com/rcorpin/woosimPrinter" />      
     </platform>          
</plugin>